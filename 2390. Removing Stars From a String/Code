class Solution {
    public String removeStars(String s) {
        StringBuilder sb = new StringBuilder();
        for(char c: s.toCharArray()){
            if(c == '*'){
                sb.setLength(sb.length() - 1);
            }else{
                sb.append(c);
            }
        }
        String ans = sb.toString();
        return ans;
    }
}
